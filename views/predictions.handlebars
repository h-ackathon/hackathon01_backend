<div id="main">
    <div id="header">
      <div id="logo">
          <a href="index.html"><img src="/images/logo_transparent.png" /></a>

        <div class="login-links">
          <a href="/login-or-register/back">Login or Register</a>
        </div>

      </div>
      <div id="menubar">
        <ul id="menu">
          {{#if isT20}}
            <li class="selected"><a href="/matches/t20">T20I</a></li>
          {{else}}
            <li><a href="/matches/t20">T20I</a></li>
          {{/if}}
          {{#if isOdi}}
            <li class="selected"><a href="/matches/odi">ODI</a></li>
          {{else}}
            <li><a href="/matches/odi">ODI</a></li>
          {{/if}}
          {{#if isTest}}
            <li class="selected"><a href="/matches/test">TEST</a></li>
          {{else}}
            <li><a href="/matches/test">TEST</a></li>
          {{/if}}
          <li><a href="/learn-more">Learn More</a></li>
        </ul>
      </div>
    </div>
    <div id="site_content">

      {{#if isT20}}

        {{#each data}}

            <div class="hm-row">
              
                <h2>{{getMatchTitle this 't20'}}</h2>
            
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 't20'}}/t20/match">Match predictions</a>
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 't20'}}/t20/over">Over predictions</a>
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 't20'}}/t20/ball">Ball predictions</a>

            </div>

        {{/each}}

      {{/if}}

      {{#if isOdi}}

        {{#each data}}

            <div class="hm-row">
              
                <h2>{{getMatchTitle this 'odi'}}</h2>
            
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 'odi'}}/odi/match">Match predictions</a>
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 'odi'}}/odi/over">Over predictions</a>
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 'odi'}}/odi/ball">Ball predictions</a>

            </div>

        {{/each}}

      {{/if}}

      {{#if isTest}}

        {{#each data}}

            <div class="hm-row">
              
                <h2>{{getMatchTitle this 'test'}}</h2>
            
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 'test'}}/t20/test">Match predictions</a>
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 'test'}}/t20/test">Over predictions</a>
                <a class="pr-button" href="/play-predictions/{{getMatchTitleWithDashes this 'test'}}/t20/test">Ball predictions</a>

            </div>

        {{/each}}

      {{/if}}

        

    </div>
